ResetData.MouseButton1Down:connect(function()
if isfile("VexHubSettings.txt") then
delfile("VexHubSettings.txt")
end
if isfile(The.path.FL) then
delfile(The.path.FL)
end
ResetData.Text = "Data Resets Successfully!"
end)

NoAnimation.MouseButton1Down:connect(function()
_G.settingsTable.AnimationDelay = 0
saveSettings()
getgenv().AnimationDelay = 0
NoAnimation.Text = "Set AnimationDelay To 0"
end)
