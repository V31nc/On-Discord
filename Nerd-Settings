_G.settingsTable = {
UIScale = "1.05";
ButtonTopLeft = true;
Wallpaper = "11926598891";
WhiteBackground = true;
OpenButtonFloating = false;
EnabledNotifications = true;
AnimationDelay = 0.6;
}

for i,v in pairs(_G.settingsTable) do 
end

local filename = "VexHubSettings.txt";

function loadSettings()
local HttpService = game:GetService("HttpService")
if (readfile and isfile and isfile(filename)) then
_G.settingsTable = HttpService:JSONDecode(readfile(filename));
for i,v in pairs(_G.settingsTable) do 
end
end
end

function saveSettings()
local json;
local HttpService = game:GetService("HttpService"); 
if (writefile) then
json = HttpService:JSONEncode(_G.settingsTable);
writefile(filename, json);
end
end

loadSettings()
wait()

-- (VOID) : Gui to Lua
-- Version: 1.4

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local NerdSettings = Instance.new("ImageLabel")
local UICorner = Instance.new("UICorner")
local NerdSettingsText = Instance.new("TextLabel")
local ResetData = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")
local UIStroke = Instance.new("UIStroke")
local NoAnimation = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")
local UIStroke_2 = Instance.new("UIStroke")
local The = loadstring(game:HttpGet("https://raw.githubusercontent.com/Forbidennstuffs/SumStuff/main/Configuration"))()

--Properties:

